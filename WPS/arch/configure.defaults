###########################################################
#ARCH	PC Linux i486 i586 i686, g95 compiler,	serial, NO GRIB2
#
COMPRESSION_LIBS	=
COMPRESSION_INC		=
FDEFS			=
FC              =       g95
SFC             =       g95
FFLAGS          =       -ffree-form -g
F77FLAGS        =       -ffixed-form -g
FNGFLAGS	=	$(FFLAGS)
LDFLAGS         =       -g
CC              =       gcc
SCC             =       gcc
CFLAGS          =       -g
CPP             =       /usr/bin/cpp -C -P -traditional
CPPFLAGS        =       -D_DOUBLEUNDERSCORE -DBYTESWAP -DLINUXG95 -DIO_NETCDF -DBIT32

